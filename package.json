{
  "name": "falcon-hackathon",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.1.20",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --fix",
    "postinstall": "bun run check",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@unocss/reset": "^0.61.5",
    "bits-ui": "^0.21.12",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.416.0",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.2",
    "@fontsource/fira-mono": "^4.5.10",
    "@sveltejs/adapter-vercel": "^5.4.1",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@unocss/eslint-plugin": "^0.61.5",
    "@unocss/extractor-svelte": "^0.61.5",
    "eslint-plugin-svelte": "^2.43.0",
    "svelte": "^5.0.0-next.199",
    "svelte-check": "^3.8.4",
    "typescript": "^5.5.4",
    "unocss": "^0.61.5",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-shadcn": "^0.3.1",
    "vite": "^5.3.5"
  }
}
